{
  "compilerOptions": {
    "sourceMap": true,
    "lib": ["ES2019", "ES2020", "DOM", "WebWorker"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "target": "ES2018",
    "baseUrl": "./",
    "jsx": "preserve",
    "allowJs": true,
    "checkJs": false,
    "strict": true,
  },
  "exclude": [
    "node_modules",
    "**/*.dts.tsx"
  ]
}
